<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="test" verbose="1">
    <test name="some tests" parallel="methods" thread-count="4">
        <groups>
            <run>
                <include name="test"/>
                <exclude name="unstable"/>
            </run>
        </groups>
        <packages>
            <package name="ui.*"/>
        </packages>
    </test>
</suite>